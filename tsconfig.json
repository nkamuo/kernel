{
    "compilerOptions": {
        //   "target": "es5",
        "target": "es2019",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts",
    ],
    "exclude": [
        "node_modules"
    ]
}
